name: WorldSystem
main: world.dev.toxi.WorldSystem
authors: [ CrazyCloudCraft ]
version: ${version}
description: ${description}
api-version: 1.13
load: STARTUP
folia-supported: false
depend: [ FAWE, WorldEdit, Vault ]
softdepend: [ PlaceholderAPI ]
commands:
  ws:
    description: WorldSystem command
permissions:
  ws.*:
    default: op
    description: Grants all /worldsystem subcommands
    children:
      ws.tp.*: true
      ws.get: true
      ws.sethome: true
      ws.build: true
      ws.gamemode: true
      ws.delete: true
      ws.lag: true
      ws.big: true
      ws.large: true
 ws.tp.*:
    default: op
    description: Grants all /worldsystem tp subcommands
    children:
      ws.tp.world: true
      ws.tp.toother: true
      ws.tp.other: true
